<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/prefs_main_template/macros/master"
      i18n:domain="pas.plugins.velruse">

<body>

<div metal:fill-slot="prefs_configlet_main">

<script type="text/javascript">
<!--

(function($){

	$(document).ready(function() {
	

	});

})(jQuery);

//-->
</script>

    <a href=""
       id="setup-link"
       tal:attributes="href string:$portal_url/plone_control_panel"
       i18n:domain="plone"
       i18n:translate="">
        Site Setup
    </a>

    <h1 class="documentFirstHeading" tal:content="view/label">View Title</h1>

    <div metal:use-macro="context/global_statusmessage/macros/portal_message">
      Portal status message
    </div>

    <div id="content-core">
        
        <ul class="formTabs">
            <li class="formTab firstFormTab">
                <a id="fieldsetlegend-default" href="#fieldsetlegend-default">
                    <span>Default</span>
                </a>
            </li>
            <li class="formTab lastFormTab">
                <a id="fieldsetlegend-culo" href="#fieldsetlegend-culo">
                    <span>Culo</span>
                </a>
            </li>
        </ul>
        <div class="visualClear"><!-- --></div>
        
        <dl class="enableFormTabbing">
        <dd id="fieldset-default" class="formPanel">
            <div id="layout-contents">
                <span tal:replace="structure view/contents" />
            </div>
        </dd>
        <dd id="fieldset-culo" class="formPanel xxxhidden">        
            <ul tal:define="plugins view/velruse_plugins"
			    tal:condition="plugins">
            	<li tal:repeat="plugin plugins">
            		<a href=""
					   tal:attributes="href string:${plugin/absolute_url}/@@manage-velruse-plugin"
					   tal:content="plugin/title">Tuitter</a>
				</li>
			</ul>
        </dd>
        </dl>
    </div>

</div>
</body>
</html>
